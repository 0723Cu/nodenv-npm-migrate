{
  "name": "nodenv-npm-migrate",
  "version": "0.1.0",
  "private": true,
  "description": "Migrate npm packages from one Node version to another",
  "homepage": "https://github.com/nodenv/nodenv-npm-migrate#readme",
  "license": "MIT",
  "author": "Yuu Yamashita (https://bitbucket.org/yyuu)",
  "contributors": [
    "Josh Hagins <hagins.josh@gmail.com>",
    "Jason Karns <jason@karns.name> (http://jason.karns.name)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nodenv/nodenv-npm-migrate.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-npm-migrate/issues"
  },
  "directories": {
    "bin": "bin",
    "lib": "libexec",
    "test": "test"
  },
  "scripts": {
    "publish:brew": "brew-publish $npm_package_name v$npm_package_version",
    "postversion": "git push origin master v$npm_package_version && npm run publish:brew"
  },
  "devDependencies": {
    "brew-publish": "^2.0.0"
  }
}
